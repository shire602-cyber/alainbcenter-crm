{
  "crons": [
    {
      "path": "/api/cron/run-reminders",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/expiry-sweeper",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/run-outbound-jobs",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/cron/renewal-reminders",
      "schedule": "0 */6 * * *"
    },
    {
      "path": "/api/cron/task-notifications",
      "schedule": "0 * * * *"
    }
  ],
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"]
}
